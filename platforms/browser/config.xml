<?xml version='1.0' encoding='utf-8'?>
<widget id="tech.bano.bpe" version="104" versionCode="104" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Barrett Plumbing Employee</name>
    <description>
        Barrett Plumbing employee access.
    </description>
    <author email="colin@bano.tech" href="https//bano.tech">
        Banotech Software
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
